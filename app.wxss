.container {
  height: 100%;
  padding: 30rpx 50rpx;
}

.container-title {
  width: 100%;
  display: flex;
  justify-content: center;
  margin-bottom: 30rpx;
}

.sub-container {
  width: 100vw - 100rpx;
  border: 1rpx solid #ccc;
  padding: 20rpx;
  margin-bottom: 50rpx;
}

.sub-container-title {
  width: 100%;
  display: flex;
  justify-content: center;
  font-size: 26rpx;
}

.container-opacity {
  opacity: 0.8;
}

.button-normal {
  width: 100% !important;
  margin-bottom: 20rpx;
  border: 1rpx solid #ccc;
  color: #333;
  background-color: aquamarine;
  font-size: 30rpx;
  font-weight: normal;
}

.margin-top-normal {
  margin-top: 20rpx;
}

.margin-bottom-normal {
  margin-bottom: 20rpx;
}

.margin-left-normal {
  margin-left: 20rpx;
}

.margin-right-normal {
  margin-right: 20rpx;
}
